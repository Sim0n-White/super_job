document.querySelector('#implicit_result').innerHTML =
  '<% if @result != nil && @result.present? %>' +
    '<h1>Result(s)</h1>' +
    '<table class="table table-striped table-dark wmaincol">' +
      '<thead>' +
        '<tr>' +
          '<td>Country</th>' +
          '<th>City</th>' +
          '<th>Company</th>' +
          '<th>Profession</th>' +
          '<th>Experience</th>' +
          '<th>Schedule</th>' +
          '<th>Payment</th>' +
          '<th>Valuta</th>' +
          '<th colspan="3"></th>' +
        '</tr>' +
      '</thead>' +

      '<tbody>' +
        '<% @result.each do |work| %>' +
            '<tr>' +
              '<% @result_co.each do |country| %>' +
                '<% if country.id == work.countryid %>' +
                  '<td><%= country.coname %></td>' +
                  '<% break %>' +
                '<% end %>' +
              '<% end %>' +
              '<% @result_ci.each do |city| %>' +
                '<% if city.id == work.cityid %>' +
                  '<td><%= city.ciname %></td>' +
                  '<% break %>' +
                '<% end %>' +
              '<% end %>' +
              '<td> <%= work.company %>' +
              '<% @result_wl.each do |proff| %>' +
                '<% if proff.id == work.profession %>' +
                  '<td><%= proff.woname %></td>' +
                  '<% break %>' +
                '<% end %>' +
              '<% end %>' +
              '<% @result_ex.each do |exp| %>' +
                '<% if exp.id == work.experience %>' +
                  '<td><%= exp.exname %></td>' +
                  '<% break %>' +
                '<% end %>' +
              '<% end %>' +
              '<% @result_tw.each do |wtime| %>' +
                '<% if wtime.id == work.wtime %>' +
                  '<td><%= wtime.wotime %></td>' +
                  '<% break %>' +
                '<% end %>' +
              '<% end %>' +
              '<td> <%= work.payment %>' +
              '<% @result_val.each do |val| %>' +
                '<% if val.id == work.value %>' +
                  '<td><%= val.valuena %></td>' +
                  '<% break %>' +
                '<% end %>' +
              '<% end %>' +
              '<td><%= link_to 'Show', work %></td>' +
            '</tr>' +
        '<% end %>' +
      '</tbody>' +
    '</table>' +
  '<% else %>' +
    '<h1> No result </h1>'+
  '<% end %>' +

    '<br>'

